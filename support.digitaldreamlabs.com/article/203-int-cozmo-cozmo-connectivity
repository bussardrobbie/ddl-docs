<script type="text/javascript" src="https://dhmmnd775wlnp.cloudfront.net/2cb238445f/js/apps/dist/2fa~apps~authentication~beacons~custom-fields~customers~dashboard~docs~docs-settings~docs-setup~edit~40573e4c-8cb3efe7e429427ef321.js" integrity="sha384-RKQpkHLIayOoYPKRUULBje+1Igb2WmHbp3uN3p4Vzcs4sU/C3SMXHQ03N1CwM8To" crossorigin="anonymous"></script>
<script type="text/javascript" src="https://dhmmnd775wlnp.cloudfront.net/2cb238445f/js/apps/dist/login-82006d082a390842d008.js" integrity="sha384-Ol0qLyhQj4auKscIbuFhBWxKBn8yGUH8WyMBMV1Hu4KqrJNPL7UD7dpIu7w6nOtn" crossorigin="anonymous"></script><script></script><script>      var client
      window.onload = function() {
        client = google.accounts.oauth2.initCodeClient({
          client_id: '545145119934-6fkm17aegnnb8u01557sqvsujimbgj47.apps.googleusercontent.com',
          scope: 'profile email',
